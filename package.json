{
  "name": "tud-scheduler",
  "version": "5.0.0",
  "description": "Helps you to orginize/select/overview your TU Delft Master courses",
  "main": "gulpfile.js",
  "dependencies": {
    "classnames": "2.2.5",
    "history": "2.1.0",
    "lodash": "4.11.1",
    "material-ui": "0.15.3",
    "react": "15.3.0",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.0.1",
    "react-grid-layout": "0.13.0",
    "react-markdown": "^2.4.2",
    "react-router": "2.8.1",
    "react-tap-event-plugin": "1.0.0",
    "superagent": "2.0.0"
  },
  "scripts": {
    "test": "npm run test:mocha --silent",
    "test:mocha": "mocha config/mocha/jsdom.js config/mocha/eslint.js src/**/*.spec.js",
    "test:dev": "npm run test:mocha -- --reporter min --watch",
    "lint": "eslint src;exit 0",
    "start": "webpack-dev-server --config config/webpack/dev.config.js --progress --inline",
    "build": "npm run test && npm run pack",
    "pack": "rimraf build && NODE_ENV=production webpack --config config/webpack/production.config.js --progress --colors",
    "mock-server": "mock-server config/mock-server.dev.json",
    "mock-server:test": "mock-server config/mock-server.test.json",
    "zip": "zip -j tud-scheduler.${npm_package_version}.zip build/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pouja/tudscheduler.git"
  },
  "keywords": [
    "tud",
    "delft",
    "master",
    "cs",
    "computer",
    "science",
    "react",
    "scheduler"
  ],
  "author": "Pouja Nikray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pouja/tudscheduler/issues"
  },
  "homepage": "https://github.com/Pouja/tudscheduler",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.14.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "babelify": "7.3.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "8.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.6.0",
    "eslint-plugin-jsx-a11y": "1.0.3",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.7.2",
    "jsdom": "9.5.0",
    "json-loader": "^0.5.4",
    "mocha": "3.0.2",
    "node-rest-emulator": "1.2.0",
    "react-addons-test-utils": "15.3.1",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "transfer-webpack-plugin": "0.1.4",
    "webpack": "beta",
    "webpack-dev-server": "beta",
    "webpack-replace": "1.0.0"
  }
}
